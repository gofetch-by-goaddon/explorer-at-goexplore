{
	"orders": {
		"klass": "orders",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": "{{ _id }}"
		},
		"projection": {
			"_id": 1,
			"customer_id": 1,
			"payment_method_id": 1,
			"delivery_method_id": 1,
			"status_id": 1,
			"billing_country_id": 1,
			"shipping_country_id": 1,
			"webshop_id": 1,
			"order_number": 1,
			"mail_address": 1,
			"datetime": 1,
			"billing_name": 1,
			"billing_first_name": 1,
			"billing_last_name": 1,
			"billing_address_1": 1,
			"billing_address_2": 1,
			"billing_zip_code": 1,
			"billing_city": 1,
			"billing_state": 1,
			"billing_phone": 1,
			"shipping_name": 1,
			"shipping_first_name": 1,
			"shipping_last_name": 1,
			"shipping_address_1": 1,
			"shipping_address_2": 1,
			"shipping_zip_code": 1,
			"shipping_city": 1,
			"shipping_state": 1,
			"shipping_phone": 1,
			"state": 1,
			"company_note": 1,
			"customer_note": 1,
			"price": 1,
			"currency": 1,
			"delivery_price": 1,
			"payment_price": 1
		}
	},
	"payment_methods": {
		"klass": "payment_methods",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": "{{ _id }}"
		},
		"projection": {
			"_id": 1,
			"remote_id": 1,
			"name": 1
		}
	},
	"delivery_methods": {
		"klass": "delivery_methods",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": "{{ _id }}"
		},
		"projection": {
			"_id": 1,
			"remote_id": 1,
			"name": 1
		}
	},
	"statuses": {
		"klass": "statuses",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": "{{ _id }}"
		},
		"projection": {
			"_id": 1,
			"remote_id": 1,
			"name": 1
		}
	},
	"countries": {
		"klass": "countries",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": {"$in": "{{ _ids }}"}
		},
		"projection": {
			"_id": 1,
			"name": 1
		}
	},
	"shipments": {
		"klass": "shipments",
		"query": {
			"company_id": "{{ company_id }}",
			"order_id": "{{ order_id }}"
		},
		"projection": {
			"_id": 1,
			"order_id": 1,
			"datetime": 1,
			"tracking_number": 1,
			"courier": 1
		},
		"sort": {
			"datetime": 1
		}
	},
	"order_lines": {
		"klass": "order_lines",
		"query": {
			"company_id": "{{ company_id }}",
			"order_id": "{{ order_id }}"
		},
		"projection": {
			"_id": 1,
			"order_id": 1,
			"variant_id": 1,
			"price": 1,
			"quantity": 1,
			"variant_sku": 1,
			"product_name": 1
		},
		"sort": {
			"_id": 1
		}
	},
	"variants": {
		"klass": "variants",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": {"$in": "{{ _ids }}"}
		},
		"projection": {
			"_id": 1,
			"product_id": 1,
			"variations": 1
		}
	},
	"products": {
		"klass": "products",
		"query": {
			"company_id": "{{ company_id }}",
			"_id": {"$in": "{{ _ids }}"}
		},
		"projection": {
			"_id": 1,
			"name": 1,
			"url": 1
		}
	}
}